{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 1234",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev -p 1234",
    "build": "next build",
    "format": "prettier-eslint --write \"**/*.js\" \"!.next/**\" \"!node_modules/**\" \"!out/**\"",
    "lint": "eslint .",
    "start": "next start -p 1234"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "contentful": "^7.14.6",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "fathom-client": "^3.0.0",
    "feed": "^4.2.2",
    "lodash.throttle": "^4.1.1",
    "markdown": "^0.5.0",
    "next": "^10.0.7",
    "nodemailer": "^6.4.18",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^4.3.1",
    "react-syntax-highlighter": "^15.4.3"
  },
  "devDependencies": {
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-react": "^7.21.4",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "^2.1.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.!out/**": "prettier --write"
  }
}
