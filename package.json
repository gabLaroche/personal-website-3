{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3030",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev -p 3030",
    "build": "next build && next export",
    "format": "prettier-eslint --write \"**/*.js\" \"!.next/**\" \"!node_modules/**\" \"!out/**\"",
    "lint": "eslint .",
    "start": "next start -p 8080"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "contentful": "^7.14.6",
    "cross-env": "^7.0.2",
    "fathom-client": "^3.0.0",
    "lodash.throttle": "^4.1.1",
    "mailgo": "^0.9.18",
    "next": "^10.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "^4.3.1"
  },
  "devDependencies": {
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-react": "^7.21.4",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "^2.1.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.!out/**": "prettier --write"
  }
}
